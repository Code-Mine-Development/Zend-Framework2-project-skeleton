{
  "name": "code-mine/zf-skeleton",
  "description": "Skeleton Application",
  "homepage": "http://code-mine.com/",
  "config": {
    "secure-http": false,
    "process-timeout": 5000,
    "github-oauth": {
      "github.com": "c839cc2caceece687ff8392f26a6b23ca055164c"
    }
  },
  "require": {
    "php": ">=5.5",
    "zendframework/zend-mvc": "2.6.*",
    "zendframework/zend-log": "2.6.*",
    "zendframework/zend-cache": "~2.5.3",
    "zfcampus/zf-apigility": "~1.2.1",
    "zfcampus/zf-apigility-documentation": "^1.1.0",
    "zfcampus/zf-development-mode": "~2.1.2",
    "marc-mabe/php-enum": "1.3.*",
    "ramsey/uuid": "3.1.*",
    "nicolopignatelli/valueobjects": "4.*",
    "adamiecradek/zf-simple-migrations": "~0.1",
    "ircmaxell/random-lib": "1.1.*",
    "league/tactician-logger": "^0.3.0",
    "rollbar/rollbar": "~0.9",
    "aws/aws-sdk-php-zf2": "~2.0",
    "code-mine/zf-tactician-comandquery": "1.1.*",
    "sensiolabs/security-checker": "3.0.*",
    "code-mine/tnn-ep-sdk": "dev-master",
    "code-mine/tnn-ep-zf-sdk":"dev-master"
  },
  "require-dev": {
    "zendframework/zftool": "dev-master",
    "zendframework/zend-developer-tools": "dev-master",
    "zfcampus/zf-apigility-admin": "^1.4.1",
    "zfcampus/zf-deploy": "~1.0",
    "code-mine/zf-tactician-comandquery-generator": "1.2.*"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/Application",
      "Infrastructure\\": "module/Infrastructure/src/Infrastructure"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.code-mine.com"
    }
  ]
}