{
  "name": "code-mine/tnn-zf-skeleton",
  "description": "Skeleton Application for TNN Zf services",
  "type": "project",
  "license": "proprietary",
  "support": {
    "issues": "http://jira.code-mine.com:8080"
  },
  "authors": [
    {
      "name": "Adam Grabek",
      "email": "adam.grabek@code-mine.com",
      "role": "Architect, developer, project leader"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "process-timeout": 5000,
    "autoloader-suffix": "codemine_tnn_cams"
  },
  "require": {
    "php": ">=5.6",
    "zendframework/zend-mvc": "~2.5",
    "zendframework/zend-modulemanager": "2.6.*",
    "zendframework/zend-loader": "2.5.*",
    "zendframework/zend-config": "2.5.*",
    "zendframework/zend-cache": "2.5.*",
    "zendframework/zend-log": "2.6.*",
    "zendframework/zend-view": "2.5.*",
    "zendframework/zend-serializer": "2.5.*",
    "zendframework/zend-i18n": "2.5.*",
    "zendframework/zend-i18n-resources": "2.5.*",
    "zendframework/zend-inputfilter": "2.5.*",
    "zendframework/zend-console": "2.5.*",
    "zendframework/zend-http": "2.5.*",
    "zfcampus/zf-hal": "1.3.*",
    "zfcampus/zf-api-problem": "1.1.*",
    "zfcampus/zf-content-negotiation": "1.1.*",
    "zendframework/zend-version": "2.5.*",
    "rwoverdijk/assetmanager": "^1.4",
    "sensiolabs/security-checker": "^3.0",
    "code-mine/tnn-customer-accounts-manager" : "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "5.1.*",
    "zendframework/zftool": "0.1.*",
    "zfcampus/zf-development-mode": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/Application"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http:\/\/composer.code-mine.com"
    }
  ],
  "minimum-stability": "dev"
}
